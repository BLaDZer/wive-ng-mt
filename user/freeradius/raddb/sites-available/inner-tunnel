server inner-tunnel {
listen {
       ipaddr = 127.0.0.1
       port = 18120
       type = auth
}

authorize {
	mschap
	update control {
		&Proxy-To-Realm := LOCAL
	}

	eap {
		ok = return
	}

	files
}

authenticate {
	Auth-Type MS-CHAP {
		mschap
	}

	eap
}
session {
	radutmp
}

post-auth {
	Post-Auth-Type REJECT {
		attr_filter.access_reject
		update outer.session-state {
			&Module-Failure-Message := &request:Module-Failure-Message
		}
	}
}

pre-proxy {
}

post-proxy {
	eap
}

}


DEFAULT	Suffix == ".ppp", Strip-User-Name = Yes
	Hint = "PPP",
	Service-Type = Framed-User,
	Framed-Protocol = PPP

DEFAULT	Suffix == ".slip", Strip-User-Name = Yes
	Hint = "SLIP",
	Service-Type = Framed-User,
	Framed-Protocol = SLIP

DEFAULT	Suffix == ".cslip", Strip-User-Name = Yes
	Hint = "CSLIP",
	Service-Type = Framed-User,
	Framed-Protocol = SLIP,
	Framed-Compression = Van-Jacobson-TCP-IP
